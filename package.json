{
	"name": "svetle",
	"description": "Consumer price analysis tool for Bulgarian market.",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"author": "Miroslav Rusev <miroslav.rusev@living.homes>",
	"license": "MIT",
	"resolutions": {
		"yaml": "^2.4.2",
		"picomatch": "^4.0.0"
	},
	"keywords": [
		"price-monitoring",
		"market-analysis",
		"data-visualization",
		"dashboard",
		"economics",
		"food-prices",
		"fuel-prices",
		"utility-costs",
		"market-trends",
		"price-analytics"
	],
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"dependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"autoprefixer": "^10.4.21",
		"chart.js": "^4.5.0",
		"postcss": "^8.5.6",
		"svelte": "^5.0.0",
		"svelte5-chartjs": "^1.0.0",
		"tailwindcss": "^3"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	}
}
